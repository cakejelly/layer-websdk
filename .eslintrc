{
  "extends": "airbnb",
  "rules": {
    "no-else-return": 0,
    "default-case": 0,
    "indent": [2, 2, {"SwitchCase": 1}],
    "one-var": [2, {"var": "always", "let": "always", "const": "never"}],
    "no-param-reassign": 0,
    "brace-style": 0,
    "max-len": [2, {"code": 120, "tabWidth": 2, "ignoreUrls": true, "ignoreComments": true}],
    "consistent-return": 0,
    "prefer-template": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": [2, {"args": "none"}]
  }
}